{
    "scriptFile": "__init__.py", 
    "bindings": [
      {
        "name": "blobin",
        "type": "blobTrigger",
        "direction": "in",
        "path": "Events/{blobname}.xml",
        "connection": "datalake_STORAGE"
      },
      {
        "type": "blob",
        "direction": "out",
